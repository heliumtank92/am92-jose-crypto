    },
    {
      "name": "Mehul Agarwal",
      "email": "agarwalmehul92@gmail.com",
      "url": "https://github.com/agarwalmehul"
    }
  ],
  "license": "MIT",
  "publishConfig": {
    "access": "public"
  },
  "keywords": [
    "jose",
    "encryption",
    "decryption",
    "rsa",
    "aes",
    "aes256gcm"
  ],
  "devDependencies": {
    "@babel/plugin-proposal-private-methods": "^7.18.6",
    "@compodoc/compodoc": "^1.1.21",
    "@types/node": "^20.3.2",
    "npm-run-all": "^4.1.5",
    "typescript": "^5.1.3"
  }
